mponents/home/Badge.vue';\n\nexport default defineComponent({\n  name: 'Home',\n\n  components: {\n    Card,\n    Badge,\n  },\n\n  setup() {\n    onMounted(() => {\n      const tl = gsap.timeline();\n\n      gsap.set('.Home__Card', {\n        opacity: 0,\n        y: 60,\n      });\n\n      gsap.set('.Home__Badge', {\n        opacity: 0,\n        rotate: -180,\n      });\n\n      tl.to('.Home__shadow', {\n        opacity: 0,\n        duration: 9,\n      });\n\n      tl.to('.Home__Badge', {\n        opacity: 1,\n        rotate: 0,\n        duration: 1,\n      });\n\n      tl.to('.Home__Card', {\n        opacity: 1,\n        y: 0,\n        stagger: 0.5,\n        ease: 'Power1.inOut',\n      });\n    });\n  },\n});\n</script>\n\n<template>\n  <div class=\"Home d-flex\">\n    <audio src=\"../assets/the-shire.mp3\" loop=\"loop\"></audio>\n\n    <div class=\"Home__shadow\"></div>\n\n    <Card\n      class=\"Home__Card\"\n      backgroundImage=\"https://i.imgur.com/CtPS78j.png\"\n      text=\"Smials\"\n    />\n\n    <Card\n      class=\"Home__Card\"\n      backgroundImage=\"https://i.imgur.com/Po53SbY.png\"\n      text=\"Mentor\"\n    />\n\n    <Badge class=\"Home__Badge\" />\n\n    <Card\n      class=\"Home__Card\"\n      backgroundImage=\"https://i.imgur.com/Fq499PH.png\"\n      text=\"Eventos\"\n    />\n\n    <Card\n      class=\"Home__Card\"\n      backgroundImage=\"https://i.imgur.com/hpsAnc3.png\"\n      text=\"Comisiones\"\n    />\n  </div>\n</template>\n\n<style lang=\"scss\" scoped>\n  .Home {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    width: 100vw;\n    height: 100vh;\n    padding: 30px 15px;\n    background: url(\"../assets/home-background.png\") no-repeat center fixed;\n    background-size: cover;\n\n    &__shadow {\n      position: absolute;\n      width: 100%;\n      height: 100%;\n      background: black;\n    }\n\n    &__Card {\n      margin: 0 15px;\n    }\n  }\n</style>\n"]}]}