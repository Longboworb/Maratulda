lex;\n    flex-direction: column;\n    width: 100%;\n    height: 100vh;\n    background: url(\"../assets/home-background.png\") no-repeat center fixed;\n    background-size: cover;\n    overflow: hidden;\n    background-size: cover;\n    overflow: hidden;\n    font-family: \"MedievalSharp\";\n  \n   }\n\n    .back{\n    display: flex;\n    position: absolute;\n    width: 100%;\n    \n    float: left;\n    margin-top: 5px;\n   \n   }\n\n\na {\n\nfont-family: \"MedievalSharp\";\nfont-size: 30px;\n\n}\n\n     .div1 {\n     display: flex;\n     justify-content: center ;\n     width: 100%;\n     height: 100%;\n     text-align: center;\n     margin-top: 10px;\n     \n\n     \n     }\n     .txtp1 {\n     display: flex;\n     justify-content: center ;\n     width: 70%;\n     margin-top: 5%;\n     color: rgb(93, 74, 36);\n     }\n     .div2 {\n     display: flex;\n     justify-content: center ;\n     width: 100%;\n     height: 100%;\n     flex-direction: column;\n     color: rgb(93, 74, 36);     \n     text-align: center;\n     margin-top: 50px;\n     }\n\n     .central{\n      display: flex;\n      position: relative;\n      width: 300px;\n      height: 300px;\n     margin-right: auto;\n     margin-left: auto;\n      \n\n  }\n     .img {\n       width: 100%;\n       height: 100%;\n     }\n\n\n\n   @media screen and (max-width:1279px) {\n\n.container {\n    display: flex;\n    flex-direction: column;\n    width: 100%;\n    height: 100%;\n    background: url(\"../assets/home-background.png\") no-repeat center fixed;\n    background-size: cover;\n    overflow: hidden;\n    padding: 30px 15px;\n    background-size: cover;\n    overflow: hidden;\n    font-family: \"MedievalSharp\";\n  \n   }\n   a {\n\n     font-family: \"MedievalSharp\";\n     font-size:20px;\n   }\n     .div1 {\n     display: flex;\n     justify-content: center ;\n     margin-top: 30px;\n     width: 100%;\n     height: 100%;\n     text-align: center;\n     \n     }\n     .txtp1 {\n     display: flex;\n     jus